{
  "name": "fer-projekt/laravel-antibot",
  "description": "Advanced bot protection: Honeypot + Time validation + HMAC signatures + Rate limiting + JavaScript detection + Auto form IDs (PHP 7.4+).",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/http": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/view": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/cache": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "FerProjekt\\AntiBot\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "FerProjekt\\AntiBot\\AntiBotServiceProvider"
      ]
    }
  }
}
